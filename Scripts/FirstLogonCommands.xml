<FirstLogonCommands>
    <SynchronousCommand>
        <CommandLine>cmdkey.exe /generic:${host1} /user:${username} /pass:${password}</CommandLine>
        <Description>Save RDP credentials</Description>
        <Order>1</Order>
    </SynchronousCommand>
